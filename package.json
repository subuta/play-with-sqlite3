{
  "name": "play-with-sqlite3",
  "version": "1.0.0",
  "description": "Play with sqlite3 and knex.",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "watch": "ava --watch",
    "migrate": "knex-migrate up",
    "seed": "knex seed:run",
    "migrate:down": "knex-migrate down",
    "migrate:drop": "knex-migrate down --to 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/play-with-sqlite3.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/subuta/play-with-sqlite3/issues"
  },
  "homepage": "https://github.com/subuta/play-with-sqlite3#readme",
  "dependencies": {
    "ava": "^1.4.1",
    "knex": "^0.16.5",
    "lodash": "^4.17.11",
    "sqlite3": "^4.0.8"
  },
  "devDependencies": {
    "bcrypt": "^3.0.6",
    "esm": "^3.2.25",
    "knex-migrate": "^1.7.2",
    "short-uuid": "^3.1.1"
  },
  "ava": {
    "files": [
      "lib/**/*.spec.js"
    ],
    "sources": [
      "lib/**/*.js",
      "!lib/**/*.spec.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "require": [
      "esm"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          ["module:ava/stage-4", {"modules": false}]
        ]
      }
    }
  }
}
